{
    "name": "@linkplaces/app",
    "version": "0.10.0",
    "description": "This provides Sidebar to display \"Unsorted Bookmarks\" folder in Places, as \"Temporary Links\"",
    "type": "module",
    "scripts": {
        "clean": "make clean -j",
        "clean:dist": "make clean_dist -j",
        "clean:obj": "make clean_obj -j",
        "clean:plain": "make clean_plain -j",
        "test": "ava",
        "build:copy": "node ./tools/cp_files.js --basedir './src/' --source '**/**.{json,html,svg}' --destination './__dist/' --verbose",
        "build:js:bundle:popup": "make __bundle_js_popup -j",
        "build:js:bundle:sidebar": "make __bundle_js_sidebar -j",
        "build:js:bundle:option": "make __bundle_js_options -j",
        "build:css:bundle:popup": "make __bundle_css_popup -j",
        "build:css:bundle:sidebar": "make __bundle_css_sidebar -j",
        "build:css:bundle:option": "make __bundle_css_options -j",
        "build:js:obj": "babel './__plain/' --out-dir './__obj/' --extensions=.js,.jsx --config-file ./babel.config.mjs --keep-file-extension",
        "build:js:plain:js": "node ./tools/cp_files.js --basedir './src/' --source 'src/**/*.{js,jsx}' --destination './__plain/src/' --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tetsuharuohzeki/linkplaces.git"
    },
    "author": {
        "name": "Tetsuharu OHZEKI",
        "email": "saneyuki.snyk@gmail.com",
        "url": "https://github.com/tetsuharuohzeki/"
    },
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/tetsuharuohzeki/linkplaces/issues"
    },
    "homepage": "https://github.com/tetsuharuohzeki/linkplaces",
    "dependencies": {
        "@linkplaces/background_service": "workspace:*",
        "@linkplaces/foundation": "workspace:*",
        "@linkplaces/ipc_message": "workspace:*",
        "@linkplaces/shared": "workspace:*",
        "@linkplaces/webext_types": "workspace:*",
        "option-t": "catalog:",
        "react": "catalog:"
    },
    "devDependencies": {
        "@babel/cli": "catalog:",
        "@babel/core": "catalog:",
        "@babel/preset-react": "catalog:",
        "@linkplaces/tools": "workspace:*",
        "@rollup/plugin-alias": "catalog:",
        "@rollup/plugin-babel": "catalog:",
        "@rollup/plugin-commonjs": "catalog:",
        "@rollup/plugin-node-resolve": "catalog:",
        "@rollup/plugin-replace": "catalog:",
        "@rollup/plugin-swc": "catalog:",
        "@swc/core": "catalog:",
        "postcss": "catalog:",
        "postcss-import": "catalog:",
        "rollup": "catalog:"
    }
}
